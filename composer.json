{
    "name": "webino/webino",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Peter Bacinsky",
            "email": "peter@bacinsky.sk"
        }
    ],
    "require": {
        "psr/container": "^1.0"
    },
    "require-dev": {
        "nette/tester": "1.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Webino\\": [
                "library/WebinoApp/src",
                "library/WebinoHttp/src",
                "library/WebinoConsole/src",
                "library/WebinoResponder/src",
                "library/WebinoException/src",
                "library/WebinoEventEmitter/src",
                "library/WebinoServiceContainer/src"
            ]
        }
    },
    "scripts": {
        "test": "bin/tester",
        "check": "phpcs --extensions=php,phpt",
        "fix": "phpcbf --extensions=php,phpt"
    }
}
